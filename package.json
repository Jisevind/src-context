{
  "name": "src-context",
  "version": "0.2.2",
  "description": "A CLI tool for processing and analyzing code context",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "src-context": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "npm run build && vitest run",
    "test:watch": "npm run build && vitest",
    "release": "standard-version && npm publish"
  },
  "keywords": [
    "cli",
    "context",
    "code-analysis",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.52.5",
    "chokidar": "^4.0.3",
    "clipboardy": "^5.0.0",
    "commander": "^14.0.2",
    "glob": "^11.0.3",
    "gpt-tokenizer": "^3.2.0",
    "ignore": "^7.0.5",
    "isbinaryfile": "^5.0.6",
    "strip-comments": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.6"
  }
}
